<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Mavenbird_BookingSystem::bookingsystem" title="Booking System" sortOrder="10">
                    <resource id="Mavenbird_BookingSystem::menu" title="Menu" sortOrder="10">
                        <resource id="Mavenbird_BookingSystem::bookings" title="View Bookings" sortOrder="1" />
                    </resource>
                    <resource id="Mavenbird_BookingSystem::support" title="Support" sortOrder="10">
                        <resource id="Mavenbird_BookingSystem::userguide" title="User Guide" sortOrder="10" />
                        <resource id="Mavenbird_BookingSystem::extension" title="Store Extension" sortOrder="20" />
                        <resource id="Mavenbird_BookingSystem::uvdesk" title="Ticket/Customisations" sortOrder="20" />
                        <resource id="Mavenbird_BookingSystem::services" title="Services" sortOrder="20" />
                        <resource id="Mavenbird_BookingSystem::reviews" title="Reviews" sortOrder="20" />
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
