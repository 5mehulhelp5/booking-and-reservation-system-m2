<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Catalog:etc/product_types.xsd">
    <type name="booking" label="Booking Product" modelInstance="Mavenbird\BookingSystem\Model\Product\Type\Booking" indexPriority="70" sortOrder="70" isQty="true">
        <priceModel instance="Mavenbird\BookingSystem\Model\Product\Type\Booking\Price" />
        <customAttributes>
            <attribute name="refundable" value="true"/>
        </customAttributes>
    </type>
    <composableTypes>
        <type name="booking" />
    </composableTypes>
</config>
